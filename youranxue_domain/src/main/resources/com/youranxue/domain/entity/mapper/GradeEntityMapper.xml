<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.youranxue.domain.entity.mapper.GradeEntityMapper">

  <select id="searchGradeList" parameterType="int" resultType="com.youranxue.domain.vo.MyGrade">
   select

  grade_mst.GRADE_NAME gradeName

    from

      YOURANXUE.GRADE_MST grade_mst

     where

     grade_mst.PUBLISHER_ID = #{publisherId}

    </select>
</mapper>