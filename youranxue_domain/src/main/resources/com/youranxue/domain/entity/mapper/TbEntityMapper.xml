<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.youranxue.domain.entity.mapper.TbEntityMapper">

  <select id="searchTbList" parameterType="string" resultType="com.youranxue.domain.vo.MyTb">
  select
   
  book_mst.BOOK_NAME bookName
  
    from
      
      BOOK_MST book_mst
      

    where

      book_mst.USER_ID = #{userId} 
   
    </select>
</mapper>